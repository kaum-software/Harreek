{
    "name": "kaum/harreek",
    "description": "A Laravel Boilerplate Framework",
    "keywords": [
        "kaum",
        "harreek"
    ],
    "homepage": "https://github.com/kaum/harreek",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Noah Scharrenberg",
            "email": "nscharrenberg@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kaum\\Harreek\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaum\\Harreek\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaum\\Harreek\\HarreekServiceProvider"
            ],
            "aliases": {
                "Harreek": "Kaum\\Harreek\\HarreekFacade"
            }
        }
    }
}
